{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "no-prototype-builtins": 0,
        "indent": [
            "error",
            4
        ],
        "react/no-unescaped-entities": 0,
        "no-confusing-arrow": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "class-methods-use-this": 0,
        "comma-dangle": [
            "error",
            "never"
        ],
        "linebreak-style": 0,
        "import/prefer-default-export": 0,
        "global-require": 0,
        "import/no-unresolved": 0,
        "max-len": [
            2,
            140,
            {
                "ignoreStrings": true,
                "ignoreComments": true
            }
        ]
    },
    "env": {
        "jest": true
    }
}